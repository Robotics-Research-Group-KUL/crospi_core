{
    "parameter-definitions": {
        "posx" : {
            "type" : "double",
            "default-value" : 0.1,
            "optional" : false,
            "description" : ["A description is a multi-line",
                             "array, with each element corresponding",
                             "to a line"
            ]
        },
        "posy" : {
            "type" : "double",
            "default-value" : 0.1,
            "optional" : false,
            "description" : [
                "y-coordinate of the origin"
            ]
        },        
        "posz" : {
            "type" : "double",
            "default-value" : 0.1,
            "optional" : false,
            "description" : [
                "z-coordinate of the origin"
            ]
        },
        "orientation" : {
            "type" : "array_of_doubles",
            "default-value" : [1.0, 0.0, 0.0, 0.0],
            "optional" : false,
            "description" : [
                "orientation given by a quaternion with for elements [w,x,y,z]",
                "the first element w corresponds to the real part of the quaternion (i.e. cos(angle/2)",
                "The three other elements x,y and z correspond to the vector part of the quaternion (i.e.",
                "amplitude sin(angle/2)"
            ]
        },
        "solver" : {
            "type" : "string",
            "default-value" : "qpoases",
            "optional": true,
            "description" : [
                "numerical solver to use"
            ]
        }
    }
}