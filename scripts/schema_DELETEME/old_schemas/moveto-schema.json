{
    "$schema": "http://json-schema.org/draft-06/schema",
    "$id":"moveto-schema.json",
    "title": "Move to",
    "description": "Performs a Cartesian-space linear motion to a given target pose",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "target": {
            "description": "final pose of the motion",
            "$ref": "frame.json"
        },
        "maxvel": {
            "type": "number",
            "default": 0.5
        },
        "maxacc": {
            "type": "number",
            "default": 1.0
        },
        "eqradius": {
            "type": "number",
            "default": 0.1
        },
        "etaslfile": {
            "description": "etaslfile, you normally leave this to the default",
            "type": "string",
            "default": "$[crospi_core]/scripts/etasl/moveto.lua"
        },
        "etasl": {
            "title": "eTaSL",
            "description": "optional parameters to tune eTaSL",
            "ref": "etasl.json",
            "default": {
                "$ref" : "/blackboard/default-etasl"
            }
        }
    },
    "required": [
        "target"
    ],
    "additionalProperties": false
}