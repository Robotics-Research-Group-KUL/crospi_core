{
    "$id":"etasl-task-json.json",
    "title": "etasl-task",
    "description": "Describes an etasl-task",
    "type": "object",
    "required" :[ "etasl-file","parameter-schema"],
    "additionalProperties": false,
    "properties": {
        "etasl-file": { "type":"string" },
        "parameter-schema" : {"type" : "string"},
        "input-topics" : {
            "type": "array",
            "items" : { "$ref" : "input-topic"}
        },
        "output-topics" : {
            "type": "array",
            "items" : { "$ref" : "output-topic"}
        }        
    }
}
