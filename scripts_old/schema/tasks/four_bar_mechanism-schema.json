{
    "$schema": "http://json-schema.org/draft-04/schema",
    "$id": "four_bar_mechanism-schema.json",
    "title": "Four bar mechanism",
    "description": "A small application that involves a feature variable",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "title": {
            "type":"string",
            "default":"four_bar_mechanism"
        },
        "task": {
            "type": "object",
            "properties": {
                "amplitude": {
                    "type": "number"
                },
                "frequency": {
                    "type": "number"
                },
                "function_generator": {
                    "type": "boolean"
                }
            },
            "additionalProperties": false,
            "required": [
                "amplitude",
                "frequency"
            ]
        },
        "etaslfile": {
            "description": "etaslfile, you normally leave this to the default",
            "type": "string",
            "default": "$[etasl_ros2]/scripts/etasl/four_bar_mechanism.lua",
            "interpolate": true
        },
        "etasl": {
            "title": "eTaSL",
            "description": "optional parameters to tune eTaSL",
            "$ref": "../etasl.json"
        }
    },
    "additionalProperties": false
}