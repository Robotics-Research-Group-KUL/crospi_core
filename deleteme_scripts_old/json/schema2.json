{
    "title": "move-to",
    "description": "Moves to a given location in space",
    "type": "object",
    "required" :[ "pos","orientation","solver"],
    "properties": {
        "pos": {
            "title": "pos",
            "description": "origin of frame",
            "type": "object",
            "properties" : {
                "x" : { "type" : "number" },
                "y" : { "type" : "number" },
                "z" : { "type" : "number" }
            },
            "required" : [ "x","y","z"],
            "transform_to" : "vector"
        },
        "orientation": {
            "title": "orientation",
            "description": "orientation of frame given my normalized quaternion",
            "type": "object",
            "properties" : {
                "w" : { "type" : "number" },
                "x" : { "type" : "number" },
                "y" : { "type" : "number" },
                "z" : { "type" : "number" }
            },
            "required" : [ "w","x","y","z"],
            "transform_to" : "quaternion"
        },        
        "solver": {
            "type": "string"
        },
        "topics": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
