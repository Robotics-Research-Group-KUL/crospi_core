{
    "$schema": "http://json-schema.org/draft-06/schema",
    "$id": "moveto-jointspace.json",
    "title": "moveto-jointspace",
    "description": "Move to a point in joint space",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "title":{
            "type":"string",
            "default":"moveto-jointspace"
        },
        "task": {
            "type": "object",
            "properties": {
                "jointvalues": {
                    "description": "values of the joints",
                    "type": "object",
                    "additionalProperties": {
                        "type": "number"
                    }
                },
                "units" : {
                    "description":"units, should be 'deg' or 'rad' ",
                    "type":"string",
                    "default":"rad"
                },
                "maxvel": {
                    "description": "maximum joint velocity [rad/s]",
                    "type": "number",
                    "default": 0.5
                },
                "maxacc": {
                    "description": "maximum joint acceleration [rad/s^2]",
                    "type": "number",
                    "default": 0.5
                },
                "worldmodel": {
                    "description": "JSON worldmodel describing the robot and its environment",
                    "type": "string",
                    "interpolate": true,
                    "default": "$[etasl_ros2]/scripts/worldmodels/ur10_robot.json"
                }
            },
            "additionalProperties": false,
            "required": [
                "jointvalues",
                "worldmodel"
            ]
        },
        "etaslfile": {
            "description": "etaslfile, you normally leave this to the default",
            "type": "string",
            "default": "$[etasl_ros2]/scripts/etasl/moveto-jointspace.lua",
            "interpolate": true
        },
        "etasl": {
            "title": "eTaSL",
            "description": "optional parameters to tune eTaSL",
            "$ref": "../etasl.json"
        }
    },
    "additionalProperties": false
}