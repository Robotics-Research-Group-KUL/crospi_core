{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "./taskframeconstraints.json",
    "title": "List of task frame constraints",
    "description": "A list of constraint specifications based on the task frame formalism",
    "type": "array",
    "format": "tabs",
    "items": {
        "type": "object",
        "headerTemplate": "{{self.name}}",
        "$ref" : "/schema/taskframeconstraint.json",
        "minItems" : 1
    },
    "options": {
        "disable_collapse": false,
        "disable_edit_json": false,
        "disable_array_reorder" :false,
        "collapsed": true
    }
}
