{
    "$schema": "../schema/blackboard-schema.json",
    "blackboard": {
        "default-etasl": {
            "initializer": {
                "full": true,
                "sample_time": 0.01,
                "duration": 3.0,
                "convergence_criterion": 1E-8,
                "weightfactor": 1E4
            },
            "solver": {
                "is-qpoasessolver": true,
                "nWSR": 100,
                "regularization_factor": 1E-8,
                "cputime": 100
            },
            "sample_time": 0.01,
            "outputhandlers": [
                {
                    "is-jointstateoutputhandler": true,
                    "topic-name": "velcmd"
                }
            ],
            "inputhandlers": [
                 {
                     "is-jointstateinputhandler": true,
                     "topic-name": "joint_states",
                     "depth": 1,
                     "number_of_tries": 200
                 }
            ]
        },
        "default": {
            "maxvel": 2.0,
            "maxacc": 2.0,
            "eqradius": 0.1,
            "frequency": 0.5
        },
        "robot-configuration": {
            "name" : "ur10e",
            "description": "",
            "default-task": {
                "etaslfile": "$[etasl_ros2]/scripts/etasl/default-task.lua",
                "task": {
                    "worldmodel": "$[etasl_ros2]/scripts/worldmodels/ur10_robot.json"
                }
            },
            "real_world_time": true,
            "robots": [
                {
                    "is-robotsimulator": true,
                    "name": "Simulator_UR10e",
                    "description": "Simple simulator consisting of a pure integrator",
                    "initial_values": {
                        "shoulder_pan_joint": 0,
                        "shoulder_lift_joint": -1.570796326794896619231321691639752,
                        "elbow_joint": 1.570796326794896619231321691639752,
                        "wrist_1_joint": -1.570796326794896619231321691639752,
                        "wrist_2_joint": -1.570796326794896619231321691639752,
                        "wrist_3_joint": 0
                    },
                    "ignore-non-existing-variables": false,
                    "sample_time": 0.005,
                    "input_topicname": "velcmd",
                    "output_topicname": "/joint_states"
                }
            ]
        }
    },
    "$defs": {}
}